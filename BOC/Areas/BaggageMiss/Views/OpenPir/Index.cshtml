      <td rowspan="2" colspan="3" style="text-align:center;">Delay<br><i>Chậm chuyến</i><br><input type="checkbox"></td>
      <td rowspan="2" colspan="3" style="text-align:center;">Missing<br><i>Thất lạc</i><br><input type="checkbox"></td>
      <td rowspan="2" colspan="3" style="text-align:center;">Damage<br><i>Hư hại</i><br><input type="checkbox"></td>
      <td rowspan="2" colspan="4" style="text-align:center;">Pilferage<br><i>Hao hụt trọng lượng</i><br><input type="checkbox"></td>
      <td rowspan="2" colspan="3" style="text-align:center;">Courtesy<br><i>Các TH khác</i><br><input type="checkbox" checked></td>
      <td rowspan="10" colspan="15">Baggage Tag/<i>Thẻ hành lý</i></td>
    
  </tr>
  
  <tr></tr><tr>
      <td>IT</td>
      <td colspan="3">Initial</td>
      <td>PT</td>
      <td colspan="4">Title(MR/MS)</td>
      <td>PS</td>
      <td colspan="6">Cabin Class(F/C/Y)</td>
    
  </tr><tr>
      
  </tr>
  <tr>
      <td>NM</td>
      <td colspan="31" rolspan="2">Passenger's name/</i> Tên hành khách</i> &nbsp;:<b>@Model.Data.FullName.ToString()</b></td>
    
  </tr>
  <tr>
      <td>TN</td>
      <td colspan="31">Baggage Tag Number/<i>Số thẻ hành lý</i></td>
      
  </tr>
  <tr>
      <td>CT</td>
      <td colspan="31" rowspan="2">Color and Type Description/<i> Mô tả màu sắc và chủng loại</i>&nbsp;:<b>@ViewBag.ColorType.ToString()</b></td>    
  </tr>
  <tr><td></td>
    
        
  </tr>
  <tr>
      <td>BI</td>
      <td colspan="31" rowspan="2">Brand Name/<i>Nhãn hiệu</i> </td>
  </tr>
  <tr>
    <td></td>
        
  </tr>
  <tr>

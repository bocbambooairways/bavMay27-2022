@using Newtonsoft.Json;
@using BOC.Areas.BaggageMiss.Models;
@model PirModel;



<html>
<head>
    <meta charset="UTF-8">
   <link rel="stylesheet" href="/lib/bootstrap/dist/css/bootstrap.css">
    <style>
      table.GeneratedTable {
        width: 100%;
        background-color: #ffffff;
        border-collapse: collapse;
        border-width: 2px;
        border-color: #080808;
        border-style: solid;
        color: #000000;
		font-size:10pt;
      }
      
      table.GeneratedTable td, table.GeneratedTable th {
        border-width: 2px;
        border-color: #080808;
        border-style: solid;
        padding: 4px;
        
      }
      
      table.GeneratedTable thead {
        background-color: #ffffff;
      }
	  
	   .left, .right {
            width: 25%;
            height: 100px;
        }

        .middle {
            width: 50%;
            height: 100px;
            text-align: center;
        }
        body{
            font-family:'Times New Roman';
        }
  
      </style>

  </head>
  <body>
   <div class="row">
            <div class="column left">
                <img src="/images/logo.png"  class="logo" style="width:94%;margin-top:10px;margin-left:20px;">
            </div>
            <div class="column middle">
				<p style="margin-top:20px;margin-left:25px;font-weight:bold;">
					PROPERTY IRREGULARITY REPORT (PIR)<br>
					<i>BIÊN BẢN BẤT THƯỜNG VỀ HÀNH LÝ</i>
				</p>
            </div>
			<div class="column right">
				<p style="margin-top:10px;">
					To be issued in BLOCK letters<br>
					<i>Điền thông tin bằng chữ IN HOA</i>
				</p>
            </div>
	</div>	
  <table class="GeneratedTable">
  <thead></thead>
  
  <tbody>
  <tr>
